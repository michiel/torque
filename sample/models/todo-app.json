  "flows": [
    {
      "id": "f1j2k3l4-m5n6-7890-1234-efghij678901",
      "name": "task_status_automation",
      "type": "Automation",
      "trigger": {
        "type": "EntityEvent",
        "config": {
          "entity": "task",
          "event": "AfterUpdate"
        }
      },
      "steps": [
        {
          "id": "s1k2l3m4-n5o6-7890-1234-fghijk789012",
          "name": "auto_complete_timestamp",
          "type": "DataUpdate",
          "condition": "status == 'Done' && completed_at == null",
          "configuration": {
            "update_fields": {
              "completed_at": "now()"
            }
          }
        },
        {
          "id": "s2k3l4m5-n6o7-8901-2345-ghijkl890123",
          "name": "clear_complete_timestamp",
          "type": "DataUpdate",
          "condition": "status != 'Done' && completed_at != null",
          "configuration": {
            "update_fields": {
              "completed_at": null
            }
          }
        }
      ],
      "error_handling": {
        "retry_attempts": 2,
        "retry_delay_seconds": 5,
        "on_error": "Continue"
      }
    },
    {
      "id": "f2j3k4l5-m6n7-8901-2345-fghijk789012",
      "name": "overdue_task_notification",
      "type": "Notification",
      "trigger": {
        "type": "Schedule",
        "config": {
          "cron": "0 9 * * *",
          "timezone": "UTC"
        }
      },
      "steps": [
        {
          "id": "s3k4l5m6-n7o8-9012-3456-hijklm901234",
          "name": "send_overdue_alerts",
          "type": "Notification",
          "condition": "due_date < today() && status != 'Done' && status != 'Cancelled'",
          "configuration": {
            "notification_type": "email",
            "template": "overdue_task_alert",
            "subject": "Overdue Tasks Alert - {{task.title}}"
          }
        }
      ],
      "error_handling": {
        "retry_attempts": 3,
        "retry_delay_seconds": 30,
        "on_error": "Stop"
      }
